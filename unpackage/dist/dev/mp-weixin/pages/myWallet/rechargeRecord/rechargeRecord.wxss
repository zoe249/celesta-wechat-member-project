
.content {
	margin: 20rpx 20rpx 0;
	padding: 20rpx;
}
.prize-view {
	background-color: #FFFFFF;
	border-radius: 16rpx;
}
.prize-item {
	display: flex;
	justify-content: space-between;
	margin: 10rpx 20rpx;
	border-bottom: 2rpx solid #EEEEEE;
}
.prize-time {
	font-size: 28rpx;
	line-height: 80rpx;
	color: #333333;
}
.prize-name {
	max-width: 330rpx;
	font-size: 28rpx;
	line-height: 80rpx;
	color: #999999;
}


